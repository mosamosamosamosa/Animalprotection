{
  "kind": "collectionType",
  "collectionName": "timelines",
  "info": {
    "singularName": "timeline",
    "pluralName": "timelines",
    "displayName": "Timeline"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "images": {
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ],
      "type": "media",
      "multiple": true
    },
    "content": {
      "type": "text"
    },
    "footprints": {
      "type": "integer",
      "default": 0
    },
    "user": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "plugin::users-permissions.user"
    }
  }
}
